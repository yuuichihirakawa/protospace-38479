<div class="main">
  <div class="inner">
    <div class="form__wrapper">
      <h2 class="page-heading">新規プロトタイプ投稿</h2>
        <%# 部分テンプレートでフォームを表示する %>
        <%= form_with(model: @prototype, local: true) do |form| %>
          <%= form.text_field :title, placeholder: "title" %>
          <%= form.text_area :catch_copy, placeholder: "catch_copy" %>
          <%= form.text_area :concept, placeholder: "concept" %>
          <%= form.file_field :image, placeholder: "image" %>
          <%= form.submit "保存する" %>
        <% end %>
    </div>
  </div>
</div>